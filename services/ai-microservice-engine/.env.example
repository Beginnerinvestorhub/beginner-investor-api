# AI Microservice Environment Variables
# Copy this file to .env and fill in your values

# Server Configuration
HOST=0.0.0.0
PORT=8002
DEBUG=false

# CORS Configuration
CORS_ORIGINS=https://your-domain.vercel.app,http://localhost:3000

# Database Configuration
DATABASE_URL=postgresql://username:password@your-host:5432/your-database

# Service Integration URLs
BEHAVIORAL_NUDGE_ENGINE_URL=http://localhost:8005
RISK_ENGINE_URL=http://localhost:8003
MARKET_DATA_URL=http://localhost:8001

# API Keys (if needed)
# API_KEY=your-api-key
# ALPHA_VANTAGE_API_KEY=your-alpha-vantage-key
# FINANCIAL_MODELING_PREP_API_KEY=your-fmp-key

# AI Model Configuration (if needed)
# MODEL_VERSION=latest
# MODEL_CACHE_SIZE=1000
# MAX_TOKENS=2048

# Monitoring and Logging
LOG_LEVEL=info
ENABLE_METRICS=true