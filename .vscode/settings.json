{
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js, ${capture}.d.ts, ${capture}.test.ts, ${capture}.spec.ts",
        "*.js": "${capture}.js.map, ${capture}.test.js, ${capture}.spec.js",
        "*.tsx": "${capture}.module.css, ${capture}.test.tsx, ${capture}.spec.tsx",
        "*.jsx": "${capture}.module.css, ${capture}.test.jsx, ${capture}.spec.jsx",
        "*.scss": "${capture}.css, ${capture}.module.scss",
        "*.css": "${capture}.module.css",
        "docker-compose*.yml": "docker-compose*.override.yml, .env, .env.*",
        "package.json": "package-lock.json, .babelrc, .eslintrc, .prettierrc, tsconfig*.json",
        "tsconfig.json": "tsconfig.*.json",
        "next.config.js": "next-env.d.ts, next.config.*",
        "README.md": "CONTRIBUTING.md, LICENSE, CHANGELOG.md, .gitignore",
        ".env": ".env.*, .env.example"
    },
    "explorer.compactFolders": false,
    "explorer.sortOrder": "type",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": false,
        "**/__pycache__": true,
        "**/.pytest_cache": true,
        "**/.mypy_cache": true
    },
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false
}