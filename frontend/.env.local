# Example environment variables for Next.js frontend
# Copy from .env.example and fill in your actual values

# Firebase Configuration
"NEXT_PUBLIC_API_URL": "https://backend-api.onrender.com",
    "NEXT_PUBLIC_AI_SERVICE_URL": "https://ai-microservice-engine.onrender.com",
    "NEXT_PUBLIC_MARKET_DATA_URL": "https://marketdata-ingestion.onrender.com",
    "NEXT_PUBLIC_RISK_ENGINE_URL": "https://python-risk-engine.onrender.com",
    "NEXT_PUBLIC_PORTFOLIO_SIMULATION_URL": "https://portfolio-simulation.onrender.com",
    "NEXT_PUBLIC_FIREBASE_PROJECT_ID": "@firebase-project-id",
    "NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET": "@firebase-storage-bucket",
    "NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID": "@firebase-messaging-sender-id",
    "NEXT_PUBLIC_FIREBASE_APP_ID": "@firebase-app-id",
    "NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID": "@firebase-measurement-id"

# API base URL for backend
NEXT_PUBLIC_API_BASE_URL=http://localhost:4000

# Market Data APIs (add your actual API keys)
NEXT_PUBLIC_ALPHA_VANTAGE_API_KEY=
NEXT_PUBLIC_IEX_CLOUD_API_KEY=

# Google Analytics ID
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=

# Stripe publishable key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=

# Stripe Price ID (for subscription checkout)
NEXT_PUBLIC_STRIPE_PRICE_ID=

# Nudge Engine API key (if used client-side)
NUDGE_ENGINE_API_KEY=NEXTAUTH_SECRET=your-secret-key-here-replace-with-random-string
NEXT_PUBLIC_API_BASE_URL=http://localhost:3001
